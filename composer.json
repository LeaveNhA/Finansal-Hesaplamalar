{
  "name": "org.kukrer.sftw/inka",
  "version": "0.0.1",
  "type": "library",
  "description": "Calculate some financials with some parameters.",
  "keywords": [
    "Kıdem Tazminatı Hesaplama"
  ],
  "homepage": "https://github.com/LeaveNhA/INOPSIS-Hesaplamalar",
  "license": "MIT",
  "config": {
  },
  "authors": [
    {
      "name": "LeaveNhA",
      "email": "seckin.kukrer@hotmail.com"
    }
  ],
  "require": {
    "php": "~7|~8",
    "lstrojny/functional-php": "^1.15"
  },
  "require-dev": {
    "phpunit/phpunit": "~7",
    "squizlabs/php_codesniffer": "~3.0",
    "friendsofphp/php-cs-fixer": "^2.14"
  },
  "autoload": {
    "psr-0": {
      "Assetic": "src/"
    },
    "files": [
      "src/Bordro/Main.php",
      "src/Bordro/Alan/OrtakHesaplamalar.php",
      "src/Bordro/BrutNet/Main.php",
      "src/Bordro/Ihbar/Main.php",
      "src/Bordro/Kidem/Main.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Bordro\\Tests\\": "tests/Bordro"
    }
  },
  "scripts": {
    "tests": "vendor/bin/phpunit tests",
    "clear": "rm -rf vendor/"
  }
}
